<div class="image-form">

    <%= form_for Image.new(:party_id => party.id), :remote => :true do |f| %>

        
            <div class="field">
                <%= f.hidden_field :party_id %>
            </div>

            <div class="field">
                <%= f.text_field :name %>
            </div>

            <div class="field">
                <%= f.submit %>
            </div>
    <% end %>

</div>

